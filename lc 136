long long int singleNumber(int* nums, int numsSize) 
{
    int temp;
     for(int i=0;i<numsSize;i++)
    {
        for(int j=i+1;j<numsSize;j++)
       {   
           temp=nums[i];
           nums[i]=nums[j];
           nums[j]=temp;
       }
    }
    int k;
    for(int i=0;i<numsSize;i++)
    {
        for(int j=i+1;j<numsSize;j++)
       {   
        if(nums[i]!=nums[j])
        {
           k=nums[i];
        }
       }
    }
   
        printf("%d",k);
    
    return 0;
}
